  .paging ul li.paging_previous a::before { content: ""; position: absolute; display: block; top: 6px; left: 8px; width: 0px; height: 0px; border-top: 7px solid transparent; border-bottom: 7px solid transparent; border-right: 10px solid rgb(25, 106, 170); }
  .paging ul li.paging_next a::before { content: ""; position: absolute; display: block; top: 6px; left: 8px; width: 0px; height: 0px; border-top: 7px solid transparent; border-bottom: 7px solid transparent; border-left: 10px solid rgb(25, 106, 170); }
  .embed-responsive-16by9 { margin-right: 0px; }
}

